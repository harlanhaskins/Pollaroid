{
  "extends": ["standard", "standard-react"],
  "globals": {
    "fetch": true
  },
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline"
    }],
    "semi": ["error", "always"],
    "space-before-function-paren": ["error", {
      "anonymous": "never",
      "named": "never",
      "asyncArrow": "ignore"
    }],
    "react/prop-types": ["off"]
  },
  "env": {
    "jasmine": true
  }
}
